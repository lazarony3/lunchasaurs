{
  "author": "Bobby Lazarony",
  "name": "lunchasaurs",
  "description": "The lunch picker application.",
  "version": "0.0.1",
  "dependencies": {
    "browserify": "13.0.1",
    "http-server": "0.9.0",
    "jquery": "3.0.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-router": "2.4.1"
  },
  "scripts": {
    "less": "lessc ./less/base.less ./css/lunchasaurs.css",
    "build": "babel src -d lib",
    "pack": "webpack ./lib/index.js ./dist/app.bundle.js",
    "prestart": "npm run less && npm run build && npm run pack",
    "start": "http-server -p 5000"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "less": "^2.7.1",
    "webpack": "^1.13.1",
    "wolfy87-eventemitter": "^5.1.0"
  }
}
